#Quicker ls
  alias ll='ls -alF'
  alias la='ls -A'
  alias l='ls -CF'

#Quick cd to home directory
  alias ~="cd ~"

#Rails aliases
  #rails server
    alias ras='rails server'

#git aliases
  alias gpom='git push origin master'
  alias gphm='git push heroku master'
  alias gpoa='git push origin --all'
  alias g='git'

#heroku aliases
  alias h='heroku'

# Change directories
# to the dotfiles directory
  alias cddot="cd ~/Dropbox/dotfiles"
# to your current freelancing project
  alias cdfree="cd ~/Projects/Freelancing/mealapp"

#source bashrc 
  alias sb='source ~/.bashrc'
